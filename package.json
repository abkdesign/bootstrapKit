{
  "name": "standard-starterkit",
  "version": "1.0.0-alpha",
  "description": "a basic starterkit when you start a new project",
  "main": "webpack.config.js",
  "scripts": {
    "start": "webpack --config webpack.config.js --env.NODE_ENV=development",
    "start:dev": "webpack-dev-server --env.NODE_ENV=local",
    "build:prod": "webpack -p --config webpack.prod.js --env.NODE_ENV=production",
    "dev-server": "json-server -p 8081 db.json",
    "start-all": "concurrently \"npm run dev-server\" \"npm run start\"",
    "test": "jest",
    "kss": "kss --config kss-config.json"
  },
  "jest": {
    "setupFiles": [
      "./jest/setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "src/shared"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest/setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abkdesign/standard-starterkit.git"
  },
  "author": "alexander k√∏ppen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abkdesign/standard-starterkit/issues"
  },
  "homepage": "https://github.com/abkdesign/standard-starterkit#readme",
